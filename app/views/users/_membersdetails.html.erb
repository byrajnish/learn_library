
<tr> 
  <td class="py-2 px-5 m-1 text-left text-sm font-medium "><%= link_to membersdetails.name, user_path(membersdetails.id)%></td>
  <td class="py-2 px-5 m-1 text-left text-sm font-medium "><%= link_to membersdetails.email, user_path(membersdetails.id)%></td>
  <td class="py-2 px-5 m-1 text-left text-sm font-medium "><%= link_to membersdetails.contact_num, user_path(membersdetails.id)%></td>
  <td class="py-2 px-2 m-1 text-left  text-sm font-medium"><%= link_to membersdetails.address, user_path(membersdetails.id) %></td>
  <td class="my-4"><%= link_to 'Show', user_path(membersdetails),class:"bg-cyan-600  text-white text-sm font-medium py-2 px-10  rounded focus:outline-none focus:shadow-outline "%></td>
  <td class="my-4"><%= link_to 'Edit', edit_user_path(membersdetails),class:"bg-cyan-600 text-white text-sm font-medium py-2 px-10  rounded focus:outline-none focus:shadow-outline " %></td>
  <td class="my-4"><%= button_to 'Delete', membersdetails, method: :delete, data: { confirm: 'Are you sure?' }, class:"bg-cyan-600 text-white text-sm font-medium py-2 px-10  rounded focus:outline-none focus:shadow-outline "%></td>
</tr>